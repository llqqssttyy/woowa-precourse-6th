{
  "env": {
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off",
    "import/extensions": ["off"],
    "max-depth": ["error", 2],
    "max-lines-per-function": ["error", 15],
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "eqeqeq": "error",
    "no-undef": "off",
    "no-console": "warn",
    "no-dupe-args": "error",
    "no-unreachable": "error",
    "no-unused-vars": "warn"
  },
  "plugins": ["import"]
}
